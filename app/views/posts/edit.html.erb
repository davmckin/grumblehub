<h1>Add your own post!</h1>

<div class="container">
  <div class="row">
    <div class="col-xs-4">
      <%= simple_form_for @post, html: {class: "form-horizontal"} do |f| %>

        <%= f.input :body %>
        <%= f.input :picture %>
        <%= f.button :submit %>

      <% end %>
    </div>
  </div>
</div>
